<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/transparent_gray"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="385dp"
        android:orientation="vertical"
        android:layout_alignParentBottom="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/tv_cancel"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/gray_12sp_text"
                android:text="@string/quxiao"/>
            <TextView
                android:id="@+id/tv_ok"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                style="@style/blue_12sp_text"
                android:text="@string/queding"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@color/view_color" />
        <RelativeLayout
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="42dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:clickable="false"
                android:gravity="center_vertical"
                android:text="出险地点"
                style="@style/black_12sp_text"/>
            <EditText
                android:id="@+id/geographicalPosition"
                android:layout_marginRight="20dp"
                android:layout_centerVertical="true"
                android:background="@null"
                style="?android:attr/textViewStyle"
                android:layout_marginLeft="100dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="请输入出险地点"
                android:textColor="@color/black"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/startPositioning"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@mipmap/location_blue" />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@color/view_color" />
        <RelativeLayout
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="42dp">

            <TextView
                android:layout_centerVertical="true"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:clickable="false"
                android:gravity="center_vertical"
                android:text="车牌号"
                style="@style/black_12sp_text" />
            <EditText
                android:id="@+id/et_plateNumber"
                android:layout_centerVertical="true"
                android:background="@null"
                style="?android:attr/textViewStyle"
                android:layout_marginLeft="100dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:hint="请输入车牌号"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@color/view_color" />
        <RelativeLayout
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="match_parent"
            android:layout_height="42dp">

            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:gravity="center_vertical"
                android:text="厂牌车型"
                style="@style/black_12sp_text" />
            <EditText
                android:id="@+id/et_label_models"
                android:layout_centerVertical="true"
                android:background="@null"
                style="?android:attr/textViewStyle"
                android:layout_marginLeft="100dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:hint="请输入厂牌车型"/>
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/ll_other_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:background="@color/gray_back">

                <TextView
                    android:id="@+id/tv_other"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"
                    android:textColor="@color/bottom_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:text="对方信息"
                    android:textSize="12sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/bottom_text"
                    android:clickable="false"
                    android:layout_toRightOf="@+id/tv_other"
                    android:layout_centerVertical="true"
                    android:text="(填写对方信息有助于报案快赔哦!)"
                    android:textSize="8sp"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="42dp">

                <TextView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:text="对方车牌"
                    style="@style/black_12sp_text" />
                <EditText
                    android:id="@+id/et_other_plate_number"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    style="?android:attr/textViewStyle"
                    android:layout_marginLeft="100dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:hint="请输入对方车牌"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@color/view_color" />
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="42dp">

                <TextView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:text="对方车厂牌车型"
                    style="@style/black_12sp_text" />
                <EditText
                    android:id="@+id/et_other_label_models"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    style="?android:attr/textViewStyle"
                    android:layout_marginLeft="100dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:hint="请输入对方厂牌车型"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@color/view_color" />
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="42dp">

                <TextView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:text="对方姓名"
                    style="@style/black_12sp_text" />
                <EditText
                    android:id="@+id/et_other_name"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    style="?android:attr/textViewStyle"
                    android:layout_marginLeft="100dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:hint="请输入对方姓名"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@color/view_color" />
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="42dp">

                <TextView
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:text="对方手机号"
                    style="@style/black_12sp_text" />
                <EditText
                    android:id="@+id/et_other_phone_number"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    style="?android:attr/textViewStyle"
                    android:layout_marginLeft="100dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:hint="请输入对方手机号"/>
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>
